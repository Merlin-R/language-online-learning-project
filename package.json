{
  "name": "@rdme/language-online-learning-project",
  "version": "0.1.1",
  "description": "OpenSource service for learning languages online for free",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "build:webapp": "babel -d dist/webapp src/webapp -D --minified --no-comments",
    "build:server": "tsc",
    "start": "node ./dist/server/index.js",
    "start:dev": "node --inspect -r ts-node/register  ./src/server/index.ts",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts,json,xml,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts,json,xml,html,jsx,tsx,properties,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "opensource",
    "language",
    "online",
    "learning",
    "project",
    "free",
    "service"
  ],
  "author": "Merlin Reichwald",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Merlin-R/language-online-learning-project.git"
  },
  "bugs": {
    "url": "https://github.com/Merlin-R/language-online-learning-project/issues"
  },
  "homepage": "https://github.com/Merlin-R/language-online-learning-project#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@rdme/babel-plugin-jsx-ui5": "^1.0.2",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.24",
    "babel-plugin-transform-modules-ui5": "^7.0.0-rc.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "1.16.4",
    "request": "^2.88.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0"
  }
}
